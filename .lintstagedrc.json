  "*.{json,md,yml,yaml}": ["prettier --write --config config/.prettierrc --ignore-path config/.prettierignore"],
  "*.{css,scss,less}": ["prettier --write --config config/.prettierrc --ignore-path config/.prettierignore"]
}
